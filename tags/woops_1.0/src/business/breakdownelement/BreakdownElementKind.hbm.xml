<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="business.breakdownelement.BreakdownElementKind" table="BreakdownElementKind">
		<!-- identifiant d'une occurrence dans la table -->
		<id
			column="id"
			name="id"
			type="integer"
		>
			<!--identifiant auto-incrementÃ© --> 
			<generator class="identity" />
		</id>
		
		<property
			column="name"
			length="50"
			name="name"
			not-null="true"
			type="string"
		 />	
	</class>	
</hibernate-mapping>