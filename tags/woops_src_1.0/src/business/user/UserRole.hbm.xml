<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="business.user.UserRole" table="UserRole">
		<id
			column="id"
			name="id"
			type="integer"
		>
		<generator class="identity" />
		</id>
		
		<property
			column="code"
			name="code"
			type="string"
			length="10"
			not-null="true"
		 />
	</class>	
</hibernate-mapping>